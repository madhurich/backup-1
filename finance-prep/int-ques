var items = [
  { name: 'Edward', value: 21 },
  { name: 'Sharpe', value: 37 },
  { name: 'And', value: 45 },
  { name: 'The', value: -12 },
  { name: 'Magnetic', value: 13 },
  { name: 'Zeros', value: 37 }
];
function ret(arr, val){
 // var res = [];
  var res = arr.map(function(elem, index){
    if(elem.value > val){
      return elem;
    }
  })
  console.log(res);
  return res;
}

//ret(items, 30);

function fil(arr, val){
  var filtered = arr.filter(function(elem){
    return elem.value > val;
  });
  return filtered;
}

//fil(items, 30)

function mapfil(arr, val){
  var filtered = arr.map(function(elem){
    if(elem.value > val){
      return elem;
    }
  })
  return filtered;
}

mapfil(items, 30);
